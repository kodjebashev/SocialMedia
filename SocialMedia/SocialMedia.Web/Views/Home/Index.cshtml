@{
    ViewData["Title"] = "Home Page";


}

@if (TempData["Message"] != null)
{
     <div class="slideshow">
        @for (var i = 0; i < Model.ImageUrls.Count; i++)
        {
            var cssClass = i == 0 ? "slide active" : "slide";
            <div class="@cssClass" style="background-image:url('@Model.ImageUrls[i]')"></div>
        }
    </div>
   
}
